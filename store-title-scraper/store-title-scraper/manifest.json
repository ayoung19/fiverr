{
    "manifest_version": 2,
    "name": "Store Title Scraper",
    "description": "Store Title Scraper",
    "version": "1",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["js/content.js"]
        }
    ],
    "permissions": [
        "storage"
    ]
}
