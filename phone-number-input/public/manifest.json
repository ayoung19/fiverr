{
  "manifest_version": 3,

  "name": "Phone Number Input",
  "description": "Phone Number Input",
  "version": "1.0.0",

  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },

  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },

  "permissions": ["storage"],
  "host_permissions": ["https://get.icheckup.com/*"]
}
