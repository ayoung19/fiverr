{
    "manifest_version": 2,
    "name": "Tab Tracker",
    "description": "Tab Tracker",
    "version": "1",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["js/content.js"]
        }
    ],
    "background": {
        "scripts": ["js/background.js"],
        "persistent": true
    }
}
